            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="Property" referenceName="properties" variableName="instanceProp"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
